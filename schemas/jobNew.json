{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required": [
        "title",
        "salary",
        "equity",
        "companyHandle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Conservator, furniture"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 50000,
            "title": "The salary Schema",
            "examples": [
                110000
            ]
        },
        "equity": {
            "type": "number",
            "default": 0,
            "title": "The equity Schema",
            "examples": [
                "0"
            ]
        },
        "companyHandle": {
            "type": "string",
            "default": "",
            "title": "The companyHandle Schema",
            "examples": [
                "watson-davis"
            ]
        }
    },
    "examples": [{
        "title": "Conservator, furniture",
        "salary": 110000,
        "equity": "0",
        "companyHandle": "watson-davis"
    }]
}